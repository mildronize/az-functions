{
  "name": "@nammatham/core",
  "version": "2.0.0-alpha.12",
  "description": "Type-safe Serverless Library for Azure Functions and friends",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build",
    "build": "tsup src/main.ts --dts",
    "format": "prettier -w src",
    "lint": "tsc --noEmit && eslint ./src && prettier -c src",
    "lint:fix": "eslint --fix ./src && prettier -c src",
    "dev": "nodemon --watch src --ext ts --exec 'npm run build'"
  },
  "keywords": [
    "azure-functions",
    "azure"
  ],
  "author": "Thada Wangthammang",
  "license": "MIT",
  "dependencies": {
    "@hono/node-server": "^1.8.1",
    "@nammatham/azure-functions": "2.0.0-alpha.12",
    "@types/bun": "^1.0.6",
    "@types/express": "^4.17.21",
    "bun": "^1.0.28",
    "colorette": "^2.0.20",
    "hono": "^4.0.4",
    "invariant": "^2.2.4",
    "pino": "^8.17.1",
    "pino-dev": "^4.0.3",
    "tiny-invariant": "^1.3.1",
    "undici": "5.20.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thaitype/nammatham.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}