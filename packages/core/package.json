{
  "name": "nammatham",
  "version": "1.1.2-beta",
  "description": "Azure Function Nodejs Lightweight framework with Dependency Injection",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "scripts": {
    "test": "ava",
    "test:watch": "ava --watch",
    "test:coverage": "c8 --reporter=lcov ava && c8 report",
    "prepublishOnly": "npm run build",
    "build": "tsup src/main.ts --dts",
    "dev": "nodemon --watch src --ext ts --exec 'npm run build'",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "keywords": [
    "azure-functions",
    "azure",
    "framework",
    "dependency-injection",
    "inversion-of-control"
  ],
  "author": "Thada Wangthammang",
  "license": "MIT",
  "dependencies": {
    "handlebars": "^4.7.7",
    "http-status-codes": "^2.2.0",
    "inversify": "^6.0.1",
    "lodash": "^4.17.21",
    "slash": "^3.0.0",
    "strip-indent": "^3.0.0",
    "type-fest": "^3.5.3",
    "typescript": "^4.9.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mildronize/nammatham.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "ignoredByWatcher": [
      "tests/**"
    ]
  }
}