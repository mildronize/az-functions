{
  "name": "nammatham-v3",
  "version": "2.0.0-alpha.12",
  "description": "Type-safe Serverless Library for Azure Functions and friends",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.mjs",
      "default": "./dist/main.js"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build",
    "build": "tsup src/main.ts --dts --format esm,cjs",
    "format": "prettier -w src",
    "lint": "tsc --noEmit && eslint ./src && prettier -c src",
    "lint:fix": "eslint --fix ./src && prettier -w src",
    "dev": "nodemon --watch src --ext ts --exec 'npm run build'"
  },
  "keywords": [
    "azure-functions",
    "azure"
  ],
  "author": "Thada Wangthammang",
  "license": "MIT",
  "dependencies": {
    "@azure/functions": "^4.1.0",
    "@nammatham/azure-functions": "2.0.0-alpha.12",
    "@nammatham/core": "2.0.0-alpha.12",
    "@nammatham/express": "2.0.0-alpha.12",
    "hono": "^4.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thaitype/nammatham.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}