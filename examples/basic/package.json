{
  "name": "basic",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/startup.js",
  "scripts": {
    "debug": "tsc && npm run start",
    "build:func": "cross-env nammatham_env=build tsx src/startup.ts",
    "build:func:watch": "nodemon --watch src --ext ts --exec 'npm run build:func'",
    "build:watch": "tsc -w",
    "start": "func start",
    "dev": "run-p build:watch build:func:watch start"
  },
  "author": "Thada Wangthammang",
  "license": "MIT",
  "dependencies": {
    "@azure/functions": "^3.5.1",
    "inversify": "^6.0.1",
    "nammatham": "workspace:*",
    "nodemon": "^2.0.20",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "tsx": "latest",
    "typescript": "latest"
  }
}